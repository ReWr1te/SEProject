#include<stdio.h>
#include<math.h>

struct Box
{
	float x1 = -1.0;
	float x2 = 1.0;
	float y1 = -1.0;
	float y2 = 1.0;
}box;

float findMaxIn5(int m);

int main()
{
	printf("%f", findMaxIn5(5));
	getchar();
	return 0;
}

float findMaxIn5(int m)
{
	float rSmall = (sqrt(2)-1)/(sqrt(2)+1);
	return box.x1*box.x1 + (m-1)*rSmall*rSmall;
}
